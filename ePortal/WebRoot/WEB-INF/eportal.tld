 <!-- 带遮罩的网页对话框标签 -->
 <tag>
    <description>带遮罩的网页对话框标签</description>
    <name>msgdialog</name>
    <tag-class>com.eportal.tld.MsgDialogTag</tag-class>
    <!-- 标签体可以是静态HTML元素，表达式语言，但不允许出现JSP脚本 -->
    <body-content>scriptless</body-content>
    <attribute>
        <description>对话框标题文字,默认为"温馨提示"</description>
        <name>title</name>
        <required>false</required>
        <!-- 可以使用JSP表达式 -->
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <description>遮罩的高度,默认为屏幕的高度,即100%</description>
        <name>height</name>
        <required>false</required>
        <!-- 可以使用JSP表达式 -->
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <description>对话框的顶部距离,默认为100px</description>
        <name>top</name>
        <required>false</required>
        <!-- 可以使用JSP表达式 -->
        <rtexprvalue>true</rtexprvalue>
    </attribute> 
    <attribute>
        <description>对话框的宽度,默认为500px</description>
        <name>boxwidth</name>
        <required>false</required>
        <!-- 可以使用JSP表达式 -->
        <rtexprvalue>true</rtexprvalue>
    </attribute>             
    <attribute>
        <description>基本URL</description>
        <name>basepath</name>
        <required>true</required>
        <!-- 可以使用JSP表达式 -->
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <description>临时ID后缀,避免ID冲突,默认为系统时间的毫秒数</description>
        <name>tmpid</name>
        <required>false</required>
        <!-- 可以使用JSP表达式 -->
        <rtexprvalue>true</rtexprvalue>
    </attribute>         
  </tag>
  
 <!-- FLV在线播放器标签 -->
 <tag>
    <description>FLV在线播放器标签</description>
    <name>flvplayer</name>
    <tag-class>com.eportal.tld.FlvPlayerTag</tag-class>
	<!-- 标签体为空 -->    
    <body-content>empty</body-content>
    <attribute>
        <description>red5服务器应用的完整访问路径</description>
        <name>red5FullUrl</name>
        <required>true</required>
        <!-- 可以使用JSP表达式 -->
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <description>red5服务器的IP地址</description>
        <name>red5ServerIp</name>
        <required>true</required>
        <!-- 可以使用JSP表达式 -->
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <description>FLV文件名</description>
        <name>flvFilename</name>
        <required>true</required>
        <!-- 可以使用JSP表达式 -->
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <description>播放器封面图片文件,默认为red5服务器应用下的cover.jpg</description>
        <name>picFilename</name>
        <required>false</required>
        <!-- 可以使用JSP表达式 -->
        <rtexprvalue>true</rtexprvalue>
    </attribute>     
    <attribute>
        <description>播放器宽度,单位:像素,默认400</description>
        <name>width</name>
        <required>false</required>
        <!-- 可以使用JSP表达式 -->
        <rtexprvalue>true</rtexprvalue>
    </attribute>             
    <attribute>
        <description>播放器高度,单位:像素,默认300</description>
        <name>height</name>
        <required>false</required>
        <!-- 可以使用JSP表达式 -->
        <rtexprvalue>true</rtexprvalue>
    </attribute>    
  </tag>  
  
 <!-- 文本新闻列表标签 -->  
  <tag>
    <description>文本新闻列表标签</description>
    <name>textnews</name>
    <tag-class>com.eportal.tld.TextnewsTag</tag-class>
	<!-- 标签体为空 -->    
    <body-content>empty</body-content>
    <attribute>
        <description>栏目编号,多个栏目编号中间用号码分隔,如:I_001,I_002</description>
        <name>section</name>
        <required>true</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>    
    <attribute>
        <description>新闻类型:0-最新新闻 1-头条新闻 2-热点新闻 3-精彩推荐 4-所有类型</description>
        <name>newstype</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <description>显示新闻的条数</description>
        <name>number</name>
        <required>true</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <description>页号,默认为1(第一页)</description>
        <name>pageNo</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>    
     <attribute>
        <description>标题的字数</description>
        <name>titlelen</name>
        <required>true</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <description>是否要分隔符:0不需要(默认) 1需要</description>
        <name>split</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <description>前缀图片:0 为一个点字符 _.jpg之类为顺序图片 其它为指定图片</description>
        <name>prex</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <description>基本URL</description>
        <name>baseurl</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <description>日期格式，默认为无，y:年 M:月 d:日 k:时 m:分 s:秒 如：yyyy-MM-dd kk:mm:ss 需要外部提供.newslist_title与.newslist_date两个CSS样式来控制显示效果</description>
        <name>dateFormat</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute> 
  </tag>
  
