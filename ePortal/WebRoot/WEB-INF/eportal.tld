<?xml version="1.0" encoding="UTF-8" ?>

<taglib xmlns="http://java.sun.com/xml/ns/j2ee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd"
    version="2.0">
   <!-- 标签库的版本号 --> 
  <tlib-version>1.0</tlib-version>
  <!-- 标签库的默认前缀 -->
  <short-name>eportal</short-name>
  <!-- 标签库的默认URI -->
  <uri>/eportal</uri>
  
 <!-- 商品类别列表标签 -->
 <tag>
    <description>商品类别列表标签</description>
    <name>category</name>
    <tag-class>com.eportal.tld.CategoryListTag</tag-class>
	<!-- 标签体为空 -->    
    <body-content>empty</body-content>
    <attribute>
        <description>1-最新商品类别列表,2-促销商品类别列表,默认值为1</description>
        <name>listtype</name>
        <required>false</required>
        <!-- 可以使用JSP表达式 -->
        <rtexprvalue>true</rtexprvalue>
    </attribute>          
 </tag>
 
 <!-- 商品列表标签 -->
 <tag>
    <description>商品列表标签</description>
    <name>merlist</name>
    <tag-class>com.eportal.tld.MerListTag</tag-class>
	<!-- 标签体为空 -->    
    <body-content>empty</body-content>
    <attribute>
        <description>基本URL</description>
        <name>baseurl</name>
        <required>true</required>
        <!-- 可以使用JSP表达式 -->
        <rtexprvalue>true</rtexprvalue>
    </attribute>         
    <attribute>
        <description>1-最新商品列表,2-促销商品列表,默认值为1</description>
        <name>listtype</name>
        <required>false</required>
        <!-- 可以使用JSP表达式 -->
        <rtexprvalue>true</rtexprvalue>
    </attribute>    
    <attribute>
        <description>商品列表条数,默认为10条</description>
        <name>size</name>
        <required>false</required>
        <!-- 可以使用JSP表达式 -->
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <description>页号,默认为第一页</description>
        <name>pageNo</name>
        <required>false</required>
        <!-- 可以使用JSP表达式 -->
        <rtexprvalue>true</rtexprvalue>
    </attribute> 
    <attribute>
        <description>商品图片宽度,默认值94</description>
        <name>picWidth</name>
        <required>false</required>
        <!-- 可以使用JSP表达式 -->
        <rtexprvalue>true</rtexprvalue>
    </attribute>             
  </tag>
  
